ac_add_options --enable-application=mobile/android
ac_add_options --disable-eme
ac_add_options --disable-tests
ac_add_options --disable-debug
ac_add_options --disable-updater
ac_add_options --disable-crashreporter
ac_add_options --without-google-play-services
ac_add_options --with-branding=mobile/android/branding/unofficial
ac_add_options --with-android-distribution-directory=$topsrcdir/fdroid
mk_add_options 'export MOZ_ANDROID_PACKAGE_INSTALL_BOUNCER='
mk_add_options MOZ_OBJDIR=./obj-android
export MOZ_INSTALL_TRACKING=
export MOZ_NATIVE_DEVICES=
